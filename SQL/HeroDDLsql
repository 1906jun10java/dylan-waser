CREATE TABLE SUPERHERO(
HEROID INTEGER PRIMARY KEY,
HERONAME VARCHAR2(30)
);

CREATE TABLE POWERS (
POWERID INT PRIMARY KEY,
POWERNAME VARCHAR2(30)
);

CREATE SEQUENCE HEROSEQ;
CREATE SEQUENCE POWSEQ;

CREATE OR REPLACE PROCEDURE INSERTSUPERHERO(NAME IN VARCHAR2)
AS
BEGIN
INSERT INTO SUPERHERO VALUES(HEROSEQ.NEXTVAL, NAME);
COMMIT;
END;
/


CREATE TABLE MCARD(
CARD_ID INTEGER PRIMARY KEY,
CARD_NAME VARCHAR2(50)
);

CREATE TABLE COLOR(
COLOR_ID INTEGER PRIMARY KEY,
COLOR_NAME VARCHAR2(10)
);

CREATE SEQUENCE MCARDSEQ;
CREATE SEQUENCE COLORSEQ;

CREATE OR REPLACE PROCEDURE INSERTMCARD(NAME IN VARCHAR2)
AS
BEGIN
    INSERT INTO MCARD VALUES(MCARDSEQ.NEXTVAL, NAME);
COMMIT;
END;
/